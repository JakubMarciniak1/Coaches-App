@using Coaches.CommonModels
@{
    ViewData["Title"] = " Tracking Logs";
}

<div>
    <h1 class="display-4"> Tracking Logs </h1>
    <div class="w-75">
        <table class="table-striped">
            <tr>
                <th>EventDate</th>
                <th>EventTypeId</th>
                <th>UserIPAddress</th>
                <th>CoachId</th>
                <th>UpdatePageUrl</th>
            </tr>

            @foreach (TrackingLogEvent trackingLogEvent in ViewData.Model)
            {

                <tr>
                    <td>@trackingLogEvent.EventDate</td>
                    <td>@trackingLogEvent.EventTypeId</td>
                    <td>@trackingLogEvent.UserIPAddress</td>
                    <td>@trackingLogEvent.CoachId</td>
                    <td>@trackingLogEvent.UpdatePageUrl</td>
                </tr>
            }

        </table>


    </div>
</div>